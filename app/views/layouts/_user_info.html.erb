<h2>User Info</h2>
      <%= image_tag current_user.get_profile_image(100,100) %>
        <table class="table">
          <tbody>
            <tr>
              <th scope="row">name</th>
              <td><%= current_user.name %></td>
            </tr>
            <tr>
              <th scope="row">introduction</th>
              <td><%= current_user.introduction %></td>
            </tr>
          </tbody>
        </table>